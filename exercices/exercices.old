body{
    background-color: #333;
    font-family: sans-serif;
}
h1{
    text-align: center;
    color: white;
}
p{
    text-align: center;
}
#section{
    margin-left: auto;
    margin-right: auto;
    margin-top: 20%;
}
.links{
    margin: auto;
    text-align: center;
}
a{
    color: white;
    text-decoration: none;
    border: solid 2px white;
    border-radius: 25px;
    margin: 16px;
    padding: 7px;
    text-align: center;
}
a:hover{
    background-color: lightgrey;
    color: black;
}